    "sane": "yarn compile && yarn lint:fix",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn sane"
    ]
  },
  "dependencies": {
    "@yarnpkg/pnpify": "^2.0.0-rc.20"
  }
