{
  "name": "stronghold",
  "version": "1.0.0",
  "description": "Creating strongholds",
  "main": "index.js",
  "repository": "https://github.com/perran/stronghold.git",
  "author": "perran <hjelmper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "compile": "tsc --noEmit",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
